{
  "name": "obsidian-annotator",
  "version": "0.2.1",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc.js src/**/*.tsx --write",
    "lint": "eslint src --ext .ts,.tsx",
    "dev": "rollup --config rollup.config.js -w",
    "test": "jest",
    "quick-build": "rollup --config rollup.config.js --environment BUILD:production",
    "build": "npm run build-hypothesis && npm run quick-build",
    "build-hypothesis": "rimraf resources/cdn.hypothes.is/hypothesis/build && cd submodules/hypothesis-client-annotator-fork && npm i --force && npm run build && cd ../.. && move-file submodules/hypothesis-client-annotator-fork/build resources/cdn.hypothes.is/hypothesis/build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@codemirror/commands": "^0.19.0",
    "@codemirror/fold": "0.19.0",
    "@codemirror/history": "^0.19.0",
    "@codemirror/language": "^0.19.0",
    "@codemirror/matchbrackets": "^0.19.0",
    "@codemirror/panel": "^0.19.0",
    "@codemirror/rangeset": "^0.19.0",
    "@codemirror/search": "^0.19.0",
    "@codemirror/state": "^0.19.0",
    "@codemirror/view": "^0.19.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^27.0.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.37",
    "@types/pdfjs-dist": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.1.1",
    "magic-string": "^0.25.7",
    "minify": "^7.0.2",
    "move-cli": "^2.0.0",
    "move-file-cli": "^3.0.0",
    "obsidian": "^0.13.11",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-base64": "^1.0.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.5",
    "tslib": "^2.2.0",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "canvas": "^2.7.0",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^3.17.2",
    "jszip": "^3.7.1",
    "mime": "^3.0.0",
    "mock-websocket": "^0.0.7",
    "monkey-around": "^2.2.0",
    "pdfjs-dist": "^2.6.347",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-offline-iframe": "^0.3.9",
    "react-pdf-highlighter": "^3.4.0",
    "rollup-plugin-import-css": "^2.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "striptags": "^3.2.0",
    "styled-components": "^5.2.3"
  }
}
